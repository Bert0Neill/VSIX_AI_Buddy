<?xml version="1.0" encoding="utf-8"?>
<CommandTable xmlns="http://schemas.microsoft.com/VisualStudio/2005-10-18/CommandTable" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  

  <!--This is the file that defines the IDs for all the commands exposed by VisualStudio. -->
  <Extern href="stdidcmd.h" />

  <!--This header contains the command ids for the menus provided by the shell. -->
  <Extern href="vsshlids.h" />

  <!--The Commands section is where commands, menus, and menu groups are defined.
      This section uses a Guid to identify the package that provides the command defined inside it. -->
  <Commands package="guidAI_BuddyPackage">
    

	  <Groups>
		  <!-- Tools menu -->
		  <Group guid="guidAI_BuddyPackageCmdSet" id="ToolsMenuGroup" priority="0x0600">
			  <Parent guid="guidSHLMainMenu" id="IDM_VS_MENU_TOOLS" />
		  </Group>

		  <!-- Editor context menu -->
		  <Group guid="guidAI_BuddyPackageCmdSet" id="EditorContextMenuGroup" priority="0x0600">
			  <Parent guid="guidSHLMainMenu" id="IDM_VS_CTXT_CODEWIN" />
		  </Group>
	  </Groups>


	

	  <Buttons>
	  <Button guid="guidAI_BuddyPackageCmdSet" id="MainMenuCmdId" priority="0x0100" type="Button">
		  <Parent guid="guidAI_BuddyPackageCmdSet" id="ToolsMenuGroup" />
		  <Icon guid="guidImages" id="bmpPic1" />
		  <Strings>
			  <ButtonText>Prompt DeepSeek</ButtonText>
		  </Strings>
	  </Button>

	  <Button guid="guidAI_BuddyPackageCmdSet" id="cmdidHighlightedTextCommand" priority="0x0100" type="Button">
		  <Parent guid="guidAI_BuddyPackageCmdSet" id="EditorContextMenuGroup" />
		  <Icon guid="guidImages1" id="bmpPic1_Alt" />
		  <Strings>
			  <ButtonText>Generate MS Unit Test</ButtonText>
		  </Strings>
	  </Button>
		  </Buttons>

	  <!--The bitmaps section is used to define the bitmaps that are used for the commands.-->
    <Bitmaps>
    
      <Bitmap guid="guidImages" href="Resources\MainMenuCmd.png" usedList="bmpPic1, bmpPic2, bmpPicSearch, bmpPicX, bmpPicArrows, bmpPicStrikethrough" />
      <Bitmap guid="guidImages1" href="Resources\HighlightedTextCommand.png" usedList="bmpPic1_Alt, bmpPic2_Alt, bmpPicSearch_Alt, bmpPicX_Alt, bmpPicArrows_Alt, bmpPicStrikethrough_Alt" />
    </Bitmaps>
  </Commands>

  <Symbols>
    <!-- This is the package guid. -->
    <GuidSymbol name="guidAI_BuddyPackage" value="{2f259d1a-ec99-4bc7-bc55-f1540b114913}" />


	  <GuidSymbol name="guidAI_BuddyPackageCmdSet" value="{f3ae7c69-4f62-438d-a386-a11549044a8c}">
		  <IDSymbol name="ToolsMenuGroup" value="0x1021" />
		  <IDSymbol name="EditorContextMenuGroup" value="0x1022" />
		  <IDSymbol name="MainMenuCmdId" value="0x0100" />
		  <IDSymbol name="cmdidHighlightedTextCommand" value="0x0102" />
	  </GuidSymbol>

  

    <GuidSymbol name="guidImages" value="{91ccd40d-b671-431c-9418-d67968904fb3}">
      <IDSymbol name="bmpPic1" value="1" />
      <IDSymbol name="bmpPic2" value="2" />
      <IDSymbol name="bmpPicSearch" value="3" />
      <IDSymbol name="bmpPicX" value="4" />
      <IDSymbol name="bmpPicArrows" value="5" />
      <IDSymbol name="bmpPicStrikethrough" value="6" />
    </GuidSymbol>
  
    <GuidSymbol value="{9fd8af83-a0da-41ca-aea1-0479aaa141e7}" name="guidImages1">
      <IDSymbol name="bmpPic1_Alt" value="1" />
      <IDSymbol name="bmpPic2_Alt" value="2" />
      <IDSymbol name="bmpPicSearch_Alt" value="3" />
      <IDSymbol name="bmpPicX_Alt" value="4" />
      <IDSymbol name="bmpPicArrows_Alt" value="5" />
      <IDSymbol name="bmpPicStrikethrough_Alt" value="6" />
    </GuidSymbol>
  </Symbols>
</CommandTable>
